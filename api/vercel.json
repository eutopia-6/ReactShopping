{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/user",
      "dest": "https://react-shopping-flask.vercel.app/user",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    },
    {
      "src": "/",
      "dest": "/main.py"
    }
  ]
}
